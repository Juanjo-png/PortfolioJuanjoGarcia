---
import "../layouts/Layout.astro"
import Layout from "../layouts/Layout.astro";
const response = await fetch ("https://jsonplaceholder.typicode.com/posts");
const posts = await response.json()

---


<Layout>
    <h1>Mis Publicaciones</h1>
    {
        posts.map((post) => (
            <div>
                <h5>{post.userId}</h5>
                <h3>{post.title}</h3>
                <p>{post.content}</p>
            </div>
        ))
    }
</Layout>
